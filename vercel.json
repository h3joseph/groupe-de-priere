{
  "headers": [
      {
          "source": "/images/(.*)",
          "headers": [
              {
                  "key": "Cache-Control",
                  "value": "public, max-age=31536000, immutable"
              }
          ]
      },
      {
          "source": "/(.*).css",
          "headers": [
              {
                  "key": "Cache-Control",
                  "value": "public, max-age=604800, must-revalidate"
              }
          ]
      }
  ]
}